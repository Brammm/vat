{
    "name": "brammm/vat",
    "type": "library",
    "description": "A library to work with VAT numbers",
    "keywords": ["VIES","VAT","EU","EC"],
    "homepage": "https://github.com/brammm/vat",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Van der Sype",
            "email": "bram.vandersype@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Brammm\\Vat\\": "src/"
        },
